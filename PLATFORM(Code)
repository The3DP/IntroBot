-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
JAVASCRIPT (console)
-------------------------------------------------------------------------------------------------------------------------------------------------------------------------

console.log("Hello, my name is Daren");
console.log("Hola, mi nombre es Daren");
console.log("Bonjour, je m'appelle Daren");
console.log("Hallo, mein Name ist Daren");
console.log("Ciao, mi chiamo Daren");
console.log("Olá, meu nome é Daren");
console.log("Hallo, mijn naam is Daren");
console.log("Привет, меня зовут Дарен");
console.log("你好，我的名字是达伦");
console.log("こんにちは、私の名前はダレンです");
console.log("안녕하세요, 제 이름은 다렌입니다");
console.log("مرحباً، اسمي دارين");
console.log("नमस्ते, मेरा नाम डैरेन है");
console.log("Salve, nomen meum est Daren");

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
JAVA
-------------------------------------------------------------------------------------------------------------------------------------------------------------------------

public class Main {
    public static void main(String[] args) {
        System.out.println("Hello, my name is Daren");
        System.out.println("Hola, mi nombre es Daren");
        System.out.println("Bonjour, je m'appelle Daren");
        System.out.println("Hallo, mein Name ist Daren");
        System.out.println("Ciao, mi chiamo Daren");
        System.out.println("Olá, meu nome é Daren");
        System.out.println("Hallo, mijn naam is Daren");
        System.out.println("Привет, меня зовут Дарен");
        System.out.println("你好，我的名字是达伦");
        System.out.println("こんにちは、私の名前はダレンです");
        System.out.println("안녕하세요, 제 이름은 다렌입니다");
        System.out.println("مرحباً، اسمي دارين");
        System.out.println("नमस्ते, मेरा नाम डैरेन है");
        System.out.println("Salve, nomen meum est Daren");
    }
}

